{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "import-name": false,
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"]
  },
  "linterOptions": {
    "exclude": [
      "build",
      "config",
      "node_modules",
      "public",
      "scripts",
      "temp",
      "./src/serviceWorker.ts"
    ]
  }
}
